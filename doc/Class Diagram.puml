@startuml
'https://plantuml.com/class-diagram

package Question {
    abstract class Question {
        Answers : String[]
    }

    class MultipleChoice {
    }

    'class CheckboxChoice {'
    '}'

    Question --|> MultipleChoice
    'Question --|> CheckboxChoice'
}

class Survey {
    Questions : Question[]
}

Survey *--"1..*" Question


package IO {
    interface IO {
        +abstract uploadSurvey()
        +abstract getSurvey()
    }

    class IOFile {
    }

    class IODatabase {
        +uploadSurvey()
        +getSurvey()
    }

    IO --|> IOFile
    IO --|> IODatabase
}

package UI {
    interface UI {
    }

    class UICommandLine {
    }

    UI --|> UICommandLine
    UI --|> UIJavaFX
}

class Controller {
}

Controller -- Survey
Controller -- IO
Controller -- UI


@enduml